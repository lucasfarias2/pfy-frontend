{
  "name": "packlify-cloud-web",
  "version": "1.0.0",
  "type": "module",
  "main": "./src/index.ts",
  "scripts": {
    "start": "packlify-server",
    "dev": "packlify-dev-server",
    "build": "npm run build:react && npm run build:server",
    "build:server": "packlify-build-server",
    "build:react": "npm run build:react:client && npm run build:react:ssr",
    "build:react:ssr": "packlify-build-ssr",
    "build:react:client": "packlify-build-client",
    "format": "packlify-format-write",
    "format:check": "packlify-format-check",
    "lint": "packlify-lint",
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:coverage": "vitest --coverage",
    "typecheck": "tsc --noEmit"
  },
  "license": "MIT",
  "dependencies": {
    "@packlify/client": "1.0.0",
    "@packlify/core": "1.0.0",
    "@packlify/ui": "file:../../../packui/packui",
    "axios": "^1.6.2",
    "classnames": "^2.3.2",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.12.0",
    "react-router-dom": "^6.15.0",
    "serve-favicon": "^2.5.0"
  },
  "devDependencies": {
    "@packlify/config-build": "1.0.0",
    "@packlify/config-format": "1.0.0",
    "@types/cookie-parser": "^1.4.6",
    "@types/express": "^4.17.21",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@types/serve-favicon": "^2.5.6",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.53.0",
    "eslint-config-packlify": "0.1.3-alpha.1",
    "nodemon": "^3.0.1",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.5",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2",
    "vite": "^4.5.0"
  }
